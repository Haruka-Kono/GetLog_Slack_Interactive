{
    "display_information": {
        "name": "GetLog_dev",
        "description": "DM等ログ取得用です。バックアップとりたい方はご相談ください。",
        "background_color": "#02244a"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": true,
            "messages_tab_enabled": false,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "GetLog_dev",
            "always_online": false
        }
    },
    "oauth_config": {
        "redirect_urls": [
            "https://slack.com/oauth/v2/authorize"
        ],
        "scopes": {
            "user": [
                "channels:history",
                "channels:read",
                "files:read",
                "im:history",
                "im:read",
                "mpim:history",
                "users:read",
                "im:write"
            ],
            "bot": [
                "chat:write",
                "chat:write.public",
                "commands"
            ]
        }
    },
    "settings": {
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}